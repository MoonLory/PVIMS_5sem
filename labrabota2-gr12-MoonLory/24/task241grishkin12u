[1;24r[23;1Hif [ -x /usr/bin/dircolors ]; then[?25h[?25l[1;23r[1;1H[2M[1;24r[22;5Htest -r ~/.dircolors && eval "$(dircolors -b ~/.dircolors)" || eval "$(dircoo[23;1Hlors -b)"[22;1H[?25h[?25l[1;23r[23;1H
